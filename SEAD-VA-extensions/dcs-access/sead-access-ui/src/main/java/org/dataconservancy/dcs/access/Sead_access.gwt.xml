<module rename-to='sead_access'>
  <inherits name='com.google.gwt.user.User'/>
  <inherits name='com.smartgwt.SmartGwt'/>
  <inherits name='com.google.gwt.xml.XML'/>
  <inherits name='com.google.gwt.jsonp.Jsonp'/>
  <inherits name="com.google.api.gwt.oauth2.OAuth2" />
  <inherits name='org.dataconservancy.dcs.access.ui.Application'/>
  <inherits name='com.google.gwt.http.HTTP'/>
  <inherits name='com.google.gwt.user.theme.standard.Standard'/>
  <inherits name='com.google.gwt.user.theme.clean.Clean'/>



  <!-- Specify the app entry point class.                         -->
  <entry-point class='org.dataconservancy.dcs.access.client.SeadApp'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>
  <extend-configuration-property name="document.compatMode" value="BackCompat" />
  
</module>
